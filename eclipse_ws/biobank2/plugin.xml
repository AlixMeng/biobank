<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         id="biobank2"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="edu.ualberta.med.biobank.rcp.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.ualberta.med.biobank.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="biobank2.biobank2"
            name="BioBank2">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
      </product>
   </extension>
   <extension
         id="product1"
         point="org.eclipse.core.runtime.products">
      <product
            application="biobank2.biobank2"
            name="BioBank2">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
         <property
               name="appName"
               value="BioBank2">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.activities">
      <activity
            description="Preferences from Eclipse Update UI"
            id="edu.ualberta.med.biobank.activity.updateui"
            name="Update Preferences">
      </activity>
      <activityPatternBinding
            activityId="edu.ualberta.med.biobank.activity.updateui"
            pattern="org\.eclipse\.update\.ui/org\.eclipse\.update\..*PreferencePage">
      </activityPatternBinding>
      <activity
            description="Preferences from Eclipse IDE UI"
            id="edu.ualberta.med.biobank.activity.eclipseide"
            name="IDE General Preferences">
      </activity>
      <activityPatternBinding
            activityId="edu.ualberta.med.biobank.activity.eclipseide"
            pattern="org\.eclipse\.ui\.ide/org\.eclipse\.ui\.preferencePages\.Workbench">
      </activityPatternBinding>
      <activity
            id="edu.ualberta.med.biobank.activity.neweditor"
            name="Activity for New Editor action">
      </activity>
      <activityPatternBinding
            activityId="edu.ualberta.med.biobank.activity.neweditor"
            pattern="org\.eclipse\.ui\.ide/org\.eclipse\.ui\.window\.newEditor">
      </activityPatternBinding>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="edu.ualberta.med.biobank.rcp.MainPerspective"
            id="edu.ualberta.med.biobank.perspective.main"
            name="Biobank Perspective">
      </perspective>
      <perspective
            class="edu.ualberta.med.biobank.rcp.PatientsAdministrationPerspective"
            fixed="true"
            id="edu.ualberta.med.biobank.perspective.patients"
            name="Patients Administration">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="edu.ualberta.med.biobank.commands.login"
            name="Login">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.logout"
            name="Logout">
      </command>
      <command
            defaultHandler="edu.ualberta.med.biobank.handlers.QuitHandler"
            id="edu.ualberta.med.biobank.commands.quit"
            name="Quit">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.siteAdd"
            name="Add Repository Site">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.siteAdd"
            name="Add Site">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.studyAdd"
            name="Add Study">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.clinicAdd"
            name="Add Clinic">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.containerTypeAdd"
            name="Add Container Type">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.containerAdd"
            name="Add Container">
      </command>
      <command
      		id="edu.ualberta.med.biobank.commands.editSampleTypes"
      		name="Edit Sample Types">
      		</command>
      <command
            defaultHandler="edu.ualberta.med.biobank.handlers.SearchHandler"
            id="edu.ualberta.med.biobank.commands.search"
            name="Search">
      </command>
      <command
            defaultHandler="edu.ualberta.med.biobank.handlers.PatientAdministrationHandler"
            id="edu.ualberta.med.biobank.commands.patientAdmin"
            name="Patient administration">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.mainAdmin"
            name="Main Administration">
      </command>
      <category
            id="edu.ualberta.med.biobank.category.patientsAdmin"
            name="Patients Administration">
      </category>
      <command
            defaultHandler="edu.ualberta.med.biobank.handlers.OpenPatientFormHandler"
            id="edu.ualberta.med.biobank.commands.patients.openPatientForm"
            name="Open Patient Form">
         <commandParameter
               id="edu.ualberta.med.biobank.commands.patients.openPatientForm.editorId"
               name="edu.ualberta.med.biobank.commands.patients.openPatientForm.editorId"
               optional="true">
         </commandParameter>
         <commandParameter
               id="edu.ualberta.med.biobank.commands.patients.openPatientForm.viewId"
               name="edu.ualberta.med.biobank.commands.patients.openPatientForm.viewId"
               optional="true">
         </commandParameter>
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.debug.initExamples"
            name="Debug - Init Examples">
      </command>
      <command
            id="edu.ualberta.med.biobank.commands.patients.patientAdd"
            name="Add patient">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="edu.ualberta.med.biobank.handlers.LoginHandler"
            commandId="edu.ualberta.med.biobank.commands.login">
         <activeWhen>
            <reference
                  definitionId="biobank2.loggedOut">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.LogoutHandler"
            commandId="edu.ualberta.med.biobank.commands.logout">
         <activeWhen>
            <reference
                  definitionId="biobank2.loggedIn">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.SiteAddHandler"
            commandId="edu.ualberta.med.biobank.commands.siteAdd">
         <activeWhen>
            <and>
               <reference
                     definitionId="biobank2.loggedIn">
               </reference>
               <reference
                     definitionId="biobank2.mainPerpectiveSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.StudyAddHandler"
            commandId="edu.ualberta.med.biobank.commands.studyAdd">
         <activeWhen>
            <and>
               <reference
                     definitionId="biobank2.loggedIn">
               </reference>
               <reference
                     definitionId="biobank2.mainPerpectiveSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.ClinicAddHandler"
            commandId="edu.ualberta.med.biobank.commands.clinicAdd">
         <activeWhen>
            <and>
               <reference
                     definitionId="biobank2.loggedIn">
               </reference>
               <reference
                     definitionId="biobank2.mainPerpectiveSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.ContainerTypeAddHandler"
            commandId="edu.ualberta.med.biobank.commands.containerTypeAdd">
         <activeWhen>
            <and>
               <reference
                     definitionId="biobank2.loggedIn">
               </reference>
               <reference
                     definitionId="biobank2.mainPerpectiveSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.ContainerAddHandler"
            commandId="edu.ualberta.med.biobank.commands.containerAdd">
         <activeWhen>
            <and>
               <reference
                     definitionId="biobank2.loggedIn">
               </reference>
               <reference
                     definitionId="biobank2.mainPerpectiveSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.EditSampleTypesHandler"
            commandId="edu.ualberta.med.biobank.commands.editSampleTypes">
         <activeWhen>
            <reference
                  definitionId="biobank2.loggedIn">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="org.eclipse.ui.internal.handlers.NewEditorHandler"
            commandId="org.eclipse.ui.window.newEditor">
         <enabledWhen>
            <and>
               <with
                  variable="activeEditor">
                  <instanceof
                     value="org.eclipse.ui.IEditorPart">
                  </instanceof>
               </with>
               <with
                  variable="activeEditor">
                  <not>
                     <instanceof
                        value="edu.ualberta.med.biobank.forms.BiobankFormBase">
                     </instanceof>
                  </not>
               </with>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.MainAdministrationHandler"
            commandId="edu.ualberta.med.biobank.commands.mainAdmin">
         <enabledWhen>
            <reference
                  definitionId="biobank2.patientViewActive">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.OpenPatientFormHandler"
            commandId="edu.ualberta.med.biobank.commands.patients.openPatientForm">
                        <activeWhen>
            <reference
                  definitionId="biobank2.patientsPerpectiveSelected">
            </reference>
         </activeWhen>
         <enabledWhen>
            <reference
                  definitionId="biobank2.patientViewActive">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.InitExamplesHandler"
            commandId="edu.ualberta.med.biobank.commands.debug.initExamples">
         <activeWhen>
            <reference
                  definitionId="biobank2.loggedIn">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="edu.ualberta.med.biobank.handlers.PatientAddHandler"
            commandId="edu.ualberta.med.biobank.commands.patients.patientAdd">
         <activeWhen>
            <reference
                  definitionId="biobank2.patientsPerpectiveSelected">
            </reference>
         </activeWhen>
         <enabledWhen>
            <reference
                  definitionId="biobank2.patientViewActive">
            </reference>
         </enabledWhen>
      </handler>
   </extension>
   <extension
