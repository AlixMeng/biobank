   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="biobank2.mainPerpectiveSelected">
         <with
               variable="activeWorkbenchWindow.activePerspective">
            <equals
                  value="edu.ualberta.med.biobank.perspective.main">
            </equals>
         </with>
      </definition>
      <definition
            id="biobank2.patientsPerpectiveSelected">
         <with
               variable="activeWorkbenchWindow.activePerspective">
            <equals
                  value="edu.ualberta.med.biobank.perspective.patients">
            </equals>
         </with>
      </definition>
      <definition
            id="biobank2.loggedIn">
         <with
               variable="edu.ualberta.med.biobank.sourceprovider.loginState">
            <equals
                  value="loggedIn">
            </equals>
         </with>
      </definition>
      <definition
            id="biobank2.loggedOut">
         <with
               variable="edu.ualberta.med.biobank.sourceprovider.loginState">
            <equals
                  value="loggedOut">
            </equals>
         </with>
      </definition>
      <definition
        id="biobank2.debugState">
         <with
               variable="edu.ualberta.med.biobank.sourceprovider.debugState">
            <equals
                  value="debug">
            </equals>
         </with>
      </definition>
      <definition
            id="biobank2.nonDebugState">
         <with
               variable="edu.ualberta.med.biobank.sourceprovider.debugState">
            <equals
                  value="nonDebug">
            </equals>
         </with>
      </definition>
      <definition
            id="biobank2.patientViewActive">
         <with
               variable="activePartId">
            <equals
                  value="edu.ualberta.med.biobank.views.patientsAdmin">
            </equals>
         </with>
      </definition>
   </extension>
