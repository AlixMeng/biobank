                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="biobank2.patientsPerpectiveSelected">
                  </reference>
               </visibleWhen>
               <parameter
                     name="edu.ualberta.med.biobank.commands.patients.openPatientForm.editorId"
                     value="edu.ualberta.med.biobank.forms.ScanLinkEntryForm">
               </parameter>
            </command>
            <command
