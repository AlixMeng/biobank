                  icon="icons/add.png"
                  label="Add Patient"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="biobank2.patientsPerpectiveSelected">
                  </reference>
               </visibleWhen>
            </command>
            <command
