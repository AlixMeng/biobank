               label="**Debug**">
            <visibleWhen
                  checkEnabled="true">
               <reference
                     definitionId="biobank2.debugState">
               </reference>
            </visibleWhen>
            <command
                  commandId="edu.ualberta.med.biobank.commands.debug.initExamples"
                  id="edu.ualberta.med.biobank.menu.debug.initExamples"
                  label="Init Examples"
                  style="push">
            </command>
         </menu>
      </menuContribution>
