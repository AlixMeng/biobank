<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="PrinterLabelTemplate" table="PRINTER_LABEL_TEMPLATE" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the PrinterLabelTemplate class -->
		<property name="name" type="string" length="50" column="NAME" unique="true"/>
		<property name="printerName" type="string" length="50" column="PRINTER_NAME"/>
		<property name="configData" type="text" column="CONFIG_DATA"/>
		<!-- Associations mapping for the PrinterLabelTemplate class -->
			<many-to-one name="jasperTemplate" class="edu.ualberta.med.biobank.model.JasperTemplate" column="JASPER_TEMPLATE_ID" lazy="proxy" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
