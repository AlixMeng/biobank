<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="OriginInfo" table="ORIGIN_INFO" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the OriginInfo class -->
		<!-- Associations mapping for the OriginInfo class -->
		<set name="commentCollection" table="ORIGIN_INFO_COMMENT" lazy="true" cascade="delete" inverse="false">
			<cache usage="read-write" />
			<key column="ORIGIN_INFO_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Comment" column="COMMENT_ID" unique="true"/>
		</set>
		<set name="specimenCollection" lazy="true" cascade="save-update" inverse="true">
			<cache usage="read-write" />
			<key column="ORIGIN_INFO_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.Specimen"/>
		</set>
		<many-to-one name="shipmentInfo" class="edu.ualberta.med.biobank.model.ShipmentInfo" column="SHIPMENT_INFO_ID" unique="true" lazy="proxy" not-null="false" cascade="all"/>
			<many-to-one name="center" class="edu.ualberta.med.biobank.model.Center" column="CENTER_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="receiverSite" class="edu.ualberta.med.biobank.model.Site" column="RECEIVER_SITE_ID" lazy="proxy" not-null="false" cascade="none"/>
	</class>
</hibernate-mapping>
