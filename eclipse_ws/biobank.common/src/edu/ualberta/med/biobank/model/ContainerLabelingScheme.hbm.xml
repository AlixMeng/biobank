<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="ContainerLabelingScheme" table="CONTAINER_LABELING_SCHEME" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the ContainerLabelingScheme class -->
		<property name="name" type="string" length="50" column="NAME"/>
		<property name="minChars" type="integer" column="MIN_CHARS"/>
		<property name="maxChars" type="integer" column="MAX_CHARS"/>
		<property name="maxRows" type="integer" column="MAX_ROWS"/>
		<property name="maxCols" type="integer" column="MAX_COLS"/>
		<property name="maxCapacity" type="integer" column="MAX_CAPACITY"/>
		<!-- Associations mapping for the ContainerLabelingScheme class -->
	</class>
</hibernate-mapping>
