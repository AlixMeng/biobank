<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="Specimen" table="SPECIMEN" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>
		</id>
		<!-- Attributes mapping for the Specimen class -->
		<property name="inventoryId" type="string" length="100" column="INVENTORY_ID" unique="true" not-null="true"/>
		<property name="quantity" type="double" column="QUANTITY"/>
		<property name="createdAt" type="java.util.Date" column="CREATED_AT" not-null="true"/>
		<!-- Associations mapping for the Specimen class -->
			<many-to-one name="topSpecimen" class="edu.ualberta.med.biobank.model.Specimen" column="TOP_SPECIMEN_ID" lazy="proxy" not-null="false" cascade="none"/>
			<many-to-one name="collectionEvent" class="edu.ualberta.med.biobank.model.CollectionEvent" column="COLLECTION_EVENT_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="currentCenter" class="edu.ualberta.med.biobank.model.Center" column="CURRENT_CENTER_ID" lazy="proxy" not-null="false" cascade="none"/>
		<set name="dispatchSpecimenCollection" lazy="true" cascade="none" inverse="true">
			<cache usage="read-write" />
			<key column="SPECIMEN_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.DispatchSpecimen"/>
		</set>
			<many-to-one name="originalCollectionEvent" class="edu.ualberta.med.biobank.model.CollectionEvent" column="ORIGINAL_COLLECTION_EVENT_ID" lazy="proxy" not-null="false" cascade="none"/>
			<many-to-one name="specimenType" class="edu.ualberta.med.biobank.model.SpecimenType" column="SPECIMEN_TYPE_ID" lazy="proxy" not-null="true" cascade="none"/>
		<one-to-one name="specimenPosition" class="edu.ualberta.med.biobank.model.SpecimenPosition" property-ref="specimen" lazy="proxy" cascade="all"/>
		<set name="childSpecimenCollection" lazy="true" cascade="save-update" inverse="true">
			<cache usage="read-write" />
			<key column="PARENT_SPECIMEN_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.Specimen"/>
		</set>
		<set name="commentCollection" table="SPECIMEN_COMMENT" lazy="true" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="SPECIMEN_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Comment" column="COMMENT_ID" unique="true"/>
		</set>
		<set name="requestSpecimenCollection" lazy="true" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="SPECIMEN_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.RequestSpecimen"/>
		</set>
			<many-to-one name="originInfo" class="edu.ualberta.med.biobank.model.OriginInfo" column="ORIGIN_INFO_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="activityStatus" class="edu.ualberta.med.biobank.model.ActivityStatus" column="ACTIVITY_STATUS_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="processingEvent" class="edu.ualberta.med.biobank.model.ProcessingEvent" column="PROCESSING_EVENT_ID" lazy="proxy" not-null="false" cascade="none"/>
			<many-to-one name="parentSpecimen" class="edu.ualberta.med.biobank.model.Specimen" column="PARENT_SPECIMEN_ID" lazy="proxy" not-null="false" cascade="none"/>
	</class>
</hibernate-mapping>
