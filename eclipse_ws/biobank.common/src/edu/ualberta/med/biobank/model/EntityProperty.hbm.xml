<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="EntityProperty" table="ENTITY_PROPERTY" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the EntityProperty class -->
		<property name="property" type="string" length="255" column="PROPERTY"/>
		<!-- Associations mapping for the EntityProperty class -->
		<set name="entityColumnCollection" lazy="false" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="ENTITY_PROPERTY_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.EntityColumn"/>
		</set>
			<many-to-one name="propertyType" class="edu.ualberta.med.biobank.model.PropertyType" column="PROPERTY_TYPE_ID" lazy="false" not-null="true" cascade="none"/>
		<set name="entityFilterCollection" lazy="false" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="ENTITY_PROPERTY_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.EntityFilter"/>
		</set>
	</class>
</hibernate-mapping>
