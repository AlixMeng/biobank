<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="ContainerType" table="CONTAINER_TYPE" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the ContainerType class -->
		<property name="name" type="string" length="255" column="NAME" not-null="true"/>
		<property name="nameShort" type="string" length="50" column="NAME_SHORT" not-null="true"/>
		<property name="topLevel" type="boolean" column="TOP_LEVEL"/>
		<property name="defaultTemperature" type="double" column="DEFAULT_TEMPERATURE"/>
		<!-- Associations mapping for the ContainerType class -->
		<set name="specimenTypeCollection" table="CONTAINER_TYPE_SPECIMEN_TYPE" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="CONTAINER_TYPE_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.SpecimenType" column="SPECIMEN_TYPE_ID" />
		</set>
		<set name="childContainerTypeCollection" table="CONTAINER_TYPE_CONTAINER_TYPE" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="PARENT_CONTAINER_TYPE_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.ContainerType" column="CHILD_CONTAINER_TYPE_ID" />
		</set>
			<many-to-one name="activityStatus" class="edu.ualberta.med.biobank.model.ActivityStatus" column="ACTIVITY_STATUS_ID" lazy="proxy" not-null="true" cascade="none"/>
		<set name="commentCollection" table="CONTAINER_TYPE_COMMENT" lazy="true" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="CONTAINER_TYPE_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Comment" column="COMMENT_ID" unique="true"/>
		</set>
		<many-to-one name="capacity" class="edu.ualberta.med.biobank.model.Capacity" column="CAPACITY_ID" unique="true" lazy="proxy" not-null="true" cascade="all"/>
			<many-to-one name="site" class="edu.ualberta.med.biobank.model.Site" column="SITE_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="childLabelingScheme" class="edu.ualberta.med.biobank.model.ContainerLabelingScheme" column="CHILD_LABELING_SCHEME_ID" lazy="proxy" not-null="true" cascade="none"/>
		<set name="parentContainerTypeCollection" table="CONTAINER_TYPE_CONTAINER_TYPE" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="CHILD_CONTAINER_TYPE_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.ContainerType" column="PARENT_CONTAINER_TYPE_ID" />
		</set>
	</class>
</hibernate-mapping>
