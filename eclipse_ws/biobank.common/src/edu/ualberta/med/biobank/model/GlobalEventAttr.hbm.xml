<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="GlobalEventAttr" table="GLOBAL_EVENT_ATTR" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the GlobalEventAttr class -->
		<property name="label" type="string" length="50" column="LABEL" unique="true" not-null="true"/>
		<!-- Associations mapping for the GlobalEventAttr class -->
			<many-to-one name="eventAttrType" class="edu.ualberta.med.biobank.model.EventAttrType" column="EVENT_ATTR_TYPE_ID" lazy="proxy" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
