<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="EventAttr" table="EVENT_ATTR" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the EventAttr class -->
		<property name="value" type="string" length="255" column="VALUE"/>
		<!-- Associations mapping for the EventAttr class -->
			<many-to-one name="collectionEvent" class="edu.ualberta.med.biobank.model.CollectionEvent" column="COLLECTION_EVENT_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="studyEventAttr" class="edu.ualberta.med.biobank.model.StudyEventAttr" column="STUDY_EVENT_ATTR_ID" lazy="proxy" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
