<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="ReportColumn" table="REPORT_COLUMN" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the ReportColumn class -->
		<property name="position" type="integer" column="POSITION"/>
		<!-- Associations mapping for the ReportColumn class -->
			<many-to-one name="propertyModifier" class="edu.ualberta.med.biobank.model.PropertyModifier" column="PROPERTY_MODIFIER_ID" lazy="false" not-null="false" cascade="none"/>
			<many-to-one name="entityColumn" class="edu.ualberta.med.biobank.model.EntityColumn" column="COLUMN_ID" lazy="false" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
