<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="Membership" table="MEMBERSHIP" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the Membership class -->
		<!-- Associations mapping for the Membership class -->
		<set name="permissionCollection" table="MEMBERSHIP_PERMISSION" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="MEMBERSHIP_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Permission" column="PERMISSION_ID" />
		</set>
			<many-to-one name="center" class="edu.ualberta.med.biobank.model.Center" column="CENTER_ID" lazy="proxy" not-null="false" cascade="none"/>
		<set name="roleCollection" table="MEMBERSHIP_ROLE" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="MEMBERSHIP_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Role" column="ROLE_ID" />
		</set>
			<many-to-one name="study" class="edu.ualberta.med.biobank.model.Study" column="STUDY_ID" lazy="proxy" not-null="false" cascade="none"/>
			<many-to-one name="principal" class="edu.ualberta.med.biobank.model.Principal" column="PRINCIPAL_ID" lazy="proxy" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
