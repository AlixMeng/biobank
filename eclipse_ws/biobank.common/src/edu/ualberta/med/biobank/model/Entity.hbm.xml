<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="Entity" table="ENTITY" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the Entity class -->
		<property name="className" type="string" length="255" column="CLASS_NAME"/>
		<property name="name" type="string" length="255" column="NAME"/>
		<!-- Associations mapping for the Entity class -->
		<set name="reportCollection" lazy="true" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="ENTITY_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.Report"/>
		</set>
		<set name="entityPropertyCollection" lazy="true" cascade="none" inverse="false">
			<cache usage="read-write" />
			<key column="ENTITY_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.EntityProperty"/>
		</set>
	</class>
</hibernate-mapping>
