<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="DispatchSpecimen" table="DISPATCH_SPECIMEN" lazy="true" polymorphism="explicit" abstract="false" discriminator-value="DispatchAliquot">
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the DispatchSpecimen class -->
		<property name="state" type="integer" column="STATE"/>
		<!-- Associations mapping for the DispatchSpecimen class -->
			<many-to-one name="dispatch" class="edu.ualberta.med.biobank.model.Dispatch" column="DISPATCH_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="specimen" class="edu.ualberta.med.biobank.model.Specimen" column="SPECIMEN_ID" lazy="proxy" not-null="true" cascade="none"/>
		<set name="commentCollection" table="DISPATCH_SPECIMEN_COMMENT" lazy="true" cascade="delete" inverse="false">
			<cache usage="read-write" />
			<key column="DISPATCH_SPECIMEN_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Comment" column="COMMENT_ID" unique="true"/>
		</set>
	</class>
</hibernate-mapping>
