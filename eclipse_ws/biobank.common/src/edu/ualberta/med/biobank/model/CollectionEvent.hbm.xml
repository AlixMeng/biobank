<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="CollectionEvent" table="COLLECTION_EVENT" lazy="true" polymorphism="explicit" abstract="false" discriminator-value="ClinicShipment">
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the CollectionEvent class -->
		<property name="visitNumber" type="integer" column="VISIT_NUMBER" not-null="true"/>
		<!-- Associations mapping for the CollectionEvent class -->
		<set name="allSpecimenCollection" lazy="true" cascade="save-update" inverse="true">
			<cache usage="read-write" />
			<key column="COLLECTION_EVENT_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.Specimen"/>
		</set>
			<many-to-one name="patient" class="edu.ualberta.med.biobank.model.Patient" column="PATIENT_ID" lazy="proxy" not-null="true" cascade="none"/>
			<many-to-one name="activityStatus" class="edu.ualberta.med.biobank.model.ActivityStatus" column="ACTIVITY_STATUS_ID" lazy="proxy" not-null="true" cascade="none"/>
		<set name="eventAttrCollection" lazy="true" cascade="delete,save-update" inverse="true">
			<cache usage="read-write" />
			<key column="COLLECTION_EVENT_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.EventAttr"/>
		</set>
		<set name="commentCollection" table="COLLECTION_EVENT_COMMENT" lazy="true" cascade="delete" inverse="false">
			<cache usage="read-write" />
			<key column="COLLECTION_EVENT_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Comment" column="COMMENT_ID" unique="true"/>
		</set>
		<set name="originalSpecimenCollection" lazy="true" cascade="none" inverse="true">
			<cache usage="read-write" />
			<key column="ORIGINAL_COLLECTION_EVENT_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.Specimen"/>
		</set>
	</class>
</hibernate-mapping>
