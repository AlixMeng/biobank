<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="Contact" table="CONTACT" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the Contact class -->
		<property name="name" type="string" length="100" column="NAME"/>
		<property name="title" type="string" length="100" column="TITLE"/>
		<property name="mobileNumber" type="string" length="50" column="MOBILE_NUMBER"/>
		<property name="faxNumber" type="string" length="50" column="FAX_NUMBER"/>
		<property name="emailAddress" type="string" length="50" column="EMAIL_ADDRESS"/>
		<property name="pagerNumber" type="string" length="50" column="PAGER_NUMBER"/>
		<property name="officeNumber" type="string" length="50" column="OFFICE_NUMBER"/>
		<!-- Associations mapping for the Contact class -->
		<set name="studyCollection" table="STUDY_CONTACT" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="CONTACT_ID" />
			<many-to-many class="edu.ualberta.med.biobank.model.Study" column="STUDY_ID" />
		</set>
			<many-to-one name="clinic" class="edu.ualberta.med.biobank.model.Clinic" column="CLINIC_ID" lazy="proxy" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
