<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="PropertyModifier" table="PROPERTY_MODIFIER" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the PropertyModifier class -->
		<property name="name" type="text" column="NAME"/>
		<property name="propertyModifier" type="text" column="PROPERTY_MODIFIER"/>
		<!-- Associations mapping for the PropertyModifier class -->
	</class>
</hibernate-mapping>
