<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="ShipmentInfo" table="SHIPMENT_INFO" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the ShipmentInfo class -->
		<property name="receivedAt" type="java.util.Date" column="RECEIVED_AT"/>
		<property name="packedAt" type="java.util.Date" column="PACKED_AT"/>
		<property name="waybill" type="string" length="255" column="WAYBILL"/>
		<property name="boxNumber" type="string" length="255" column="BOX_NUMBER"/>
		<!-- Associations mapping for the ShipmentInfo class -->
			<many-to-one name="shippingMethod" class="edu.ualberta.med.biobank.model.ShippingMethod" column="SHIPPING_METHOD_ID" lazy="proxy" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
