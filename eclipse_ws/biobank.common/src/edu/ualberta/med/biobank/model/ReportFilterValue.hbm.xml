<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="ReportFilterValue" table="REPORT_FILTER_VALUE" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the ReportFilterValue class -->
		<property name="position" type="integer" column="POSITION"/>
		<property name="value" type="text" column="VALUE"/>
		<property name="secondValue" type="text" column="SECOND_VALUE"/>
		<!-- Associations mapping for the ReportFilterValue class -->
	</class>
</hibernate-mapping>
