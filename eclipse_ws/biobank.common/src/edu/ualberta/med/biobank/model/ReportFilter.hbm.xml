<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.ualberta.med.biobank.model" auto-import="false" default-cascade="none">
	<class name="ReportFilter" table="REPORT_FILTER" lazy="true" polymorphism="explicit" abstract="false" >
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="increment"/>			
		</id>
          <version name="version" column="VERSION" access="field" />
		<!-- Attributes mapping for the ReportFilter class -->
		<property name="position" type="integer" column="POSITION"/>
		<property name="operator" type="integer" column="OPERATOR"/>
		<!-- Associations mapping for the ReportFilter class -->
		<set name="reportFilterValueCollection" lazy="false" cascade="all" inverse="false">
			<cache usage="read-write" />
			<key column="REPORT_FILTER_ID" not-null="false" />
			<one-to-many class="edu.ualberta.med.biobank.model.ReportFilterValue"/>
		</set>
			<many-to-one name="entityFilter" class="edu.ualberta.med.biobank.model.EntityFilter" column="ENTITY_FILTER_ID" lazy="false" not-null="true" cascade="none"/>
	</class>
</hibernate-mapping>
