)

for inv_id in "${INV_IDS[@]}"
do
    mysqlCsv "
select patient.PNUMBER, sample.LINK_DATE,
date(DATE_PROCESSED) as date_received,
pv_attr.value as worksheet,
substr(container.label, 2, 1) as fnum,
substr(container.label, 3, 2) as rack,
substr(container.label, 5, 2) as box,
concat(char(abstract_position.ROW + 65),char(abstract_position.COL + 49)) as cell,
INVENTORY_ID
from sample
join patient_visit on patient_visit.id=sample.PATIENT_VISIT_ID
join patient on patient.id=patient_visit.PATIENT_ID
join pv_attr on pv_attr.PATIENT_VISIT_ID=patient_visit.id
join study_pv_attr on study_pv_attr.id=pv_attr.STUDY_PV_ATTR_ID
join sample_position on sample_position.SAMPLE_ID=sample.id
join abstract_position on abstract_position.ID=sample_position.ABSTRACT_POSITION_ID
join container on container.id=sample_position.CONTAINER_ID
where inventory_id like binary '$inv_id'
and study_pv_attr.label='Worksheet'
"
done
